{
  "name": "flipper-agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "mcp/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces --if-present",
    "ns-agent": "npm run start --workspace=flipperagents-ns-mcp",
    "tmos-agent": "npm run start --workspace=flipperagents-tmos-mcp"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
